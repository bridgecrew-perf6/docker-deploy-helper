{
  "compilerOptions": {
    "target": "ES6",
    "lib": [
      "esnext"
    ],
    "allowJs": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    // "noEmit": true,
    "esModuleInterop": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    // "isolatedModules": true,
    "skipLibCheck": true,
    "outDir": "build"
  },
  "include": [
    "src/*"
  ],
  "exclude": []
}